<?xml version="1.0" ?>
<odoo>

    <record id="paperformat_electronic_receipt" model="report.paperformat">
        <field name="name">Electronic Receipt</field>
        <field name="default" eval="True"/>
        <field name="format">Letter</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">40</field>
        <field name="margin_bottom">25</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">35</field>
        <field name="dpi">90</field>
    </record>

    <record id="invoice_gt_invoice_electronic_receip" model="ir.actions.report">
        <field name="name">Electronic Receipt</field>
        <field name="model">account.move</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">invoice_gt.invoice_electronic_receipt</field>
        <field name="report_file">invoice_gt.invoice_electronic_receipt</field>
        <field name="binding_model_id" ref="model_account_move"/>
        <field name="paperformat_id" ref="invoice_gt.paperformat_electronic_receipt" />
    </record>

